{
  "name": "ember-cli-yadda",
  "version": "1.0.0-beta.1",
  "description": "Ember-cli yadda addon for running Gherkin acceptance tests",
  "keywords": [
    "ember-addon",
    "yadda",
    "bdd",
    "acceptance testing",
    "gherkin",
    "cucumber",
    "specifications",
    "features"
  ],
  "license": "MIT",
  "author": "CURIT, remkoboschker, albertjan",
  "repository": "github.com/curit/ember-cli-yadda",
  "scripts": {
    "build": "ember build",
    "lint:js": "eslint ./*.js addon addon-test-support app config lib server test-support tests blueprints/*.js node-tests",
    "start": "ember serve",
    "test": "ember try:each",
    "nodetest": "mocha node-tests/blueprints --recursive"
  },
  "dependencies": {
    "broccoli-funnel": "^2.0.1",
    "broccoli-persistent-filter": "^1.4.3",
    "ember-auto-import": "^1.2.13",
    "ember-cli-babel": "^6.6.0",
    "yadda": "*"
  },
  "devDependencies": {
    "broccoli-asset-rev": "^2.4.5",
    "ember-cli": "~3.0.0",
    "ember-cli-blueprint-test-helpers": "^0.18.3",
    "ember-cli-dependency-checker": "^2.0.0",
    "ember-cli-eslint": "^4.2.1",
    "ember-cli-htmlbars": "^2.0.1",
    "ember-cli-htmlbars-inline-precompile": "^1.0.0",
    "ember-cli-inject-live-reload": "^1.4.1",
    "ember-cli-qunit": "^4.1.1",
    "ember-cli-shims": "^1.2.0",
    "ember-cli-sri": "^2.1.0",
    "ember-cli-uglify": "^2.0.0",
    "ember-disable-prototype-extensions": "^1.1.2",
    "ember-export-application-global": "^2.0.0",
    "ember-load-initializers": "^1.0.0",
    "ember-maybe-import-regenerator": "^0.1.6",
    "ember-resolver": "^4.0.0",
    "ember-source": "~3.0.0",
    "ember-source-channel-url": "^1.0.1",
    "ember-try": "^0.2.23",
    "eslint-plugin-ember": "^5.0.0",
    "eslint-plugin-node": "^5.2.1",
    "loader.js": "^4.2.3",
    "mocha": "^4.0.1"
  },
  "engines": {
    "node": "6.* || >= 7.*"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "before": [
      "ember-cli-babel"
    ]
  }
}
